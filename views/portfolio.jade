    video_count =1;
    videoPlayer = document.getElementById("video");    

    function run(){
        corre=true;
        video_count++;
        if (video_count == 4) video_count = 1;
        var nextVideo = "video/"+video_count+".mp4";
        videoPlayer.src = nextVideo;
        $("video").fadeIn(1000,function(){ });             
